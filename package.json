{
  "name": "window-info",
  "version": "1.0.2",
  "description": "Window Info is a readable stream that pushes data about windows on screen on MacOS.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "yarn t test/spec",
    "test-build": "BABEL_ENV=test-build yarn t test/spec",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "b": "b --copy-files --source-maps",
    "build": "yarn-s b doc",
    "doc": "doc documentary -o README.md"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/window-info.git"
  },
  "keywords": [
    "window-info",
    "window",
    "os",
    "MacOS",
    "winid"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/window-info/issues"
  },
  "homepage": "https://github.com/artdecocode/window-info#readme",
  "devDependencies": {
    "documentary": "1.8.0",
    "eslint-config-artdeco": "1.0.0",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  }
}
